// Jest Snapshot v1, https://goo.gl/fbAQLP

exports[`panic-dashboard-overview renders 1`] = `
<panic-dashboard-overview>
  <h1 class="panic-dashboard-overview__title">
    DASHBOARD OVERVIEW
  </h1>
  <svc-surface label="general">
    <svc-card class="panic-dashboard-overview__chain-card">
      <div slot="expanded">
        <svc-event-emitter debounce="100" eventname="filter-changed">
          <input hidden="" name="base-chain-name" value="general">
          <div class="panic-dashboard-overview__chain-filter-container">
            <svc-select header="Select chains" id="general_chain-filter" multiple="" name="selected-chains" placeholder="Select chains"></svc-select>
          </div>
          <div class="panic-dashboard-overview__slots">
            <div class="panic-dashboard-overview__pie-chart">
              <svc-pie-chart pie-slice-text="value" tooltip-trigger="none"></svc-pie-chart>
            </div>
            <div class="panic-dashboard-overview__data-table-container">
              <div>
                <svc-select header="Select severities" id="general_severity-filter" multiple="" name="alerts-severity" placeholder="Select severities"></svc-select>
                <svc-data-table id="general" last-clicked-column-index="1" no-records-message="There are no alerts to display at this time" ordering="descending"></svc-data-table>
              </div>
            </div>
          </div>
        </svc-event-emitter>
      </div>
      <div class="panic-dashboard-overview__collapsed-card" slot="collapsed">
        <svc-buttons-container>
          <svc-button badge="8" color="warning" icon-name="warning" icon-position="icon-only"></svc-button>
          <svc-button badge="2" color="danger" icon-name="alert-circle" icon-position="icon-only"></svc-button>
          <svc-button badge="0" color="dark" icon-name="skull" icon-position="icon-only"></svc-button>
        </svc-buttons-container>
      </div>
    </svc-card>
    <svc-label class="panic-dashboard-overview__info-message" color="dark" position="start">
      This section displays only warning, critical and error alerts. For a full report, check
      <svc-anchor label="Alerts Overview." url="#alerts-overview"></svc-anchor>
    </svc-label>
  </svc-surface>
</panic-dashboard-overview>
`;
